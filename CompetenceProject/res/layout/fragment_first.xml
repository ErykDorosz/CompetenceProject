<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/firstFragmentConstId"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1877EC"
    tools:context=".FirstFragment">


    <RelativeLayout
        android:id="@+id/Lista"
        android:layout_width="wrap_content"
        android:layout_height="692dp"
        android:background="#FFFFFF"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="30dp">

        <ExpandableListView
            android:id="@+id/expandable_list"
            android:layout_width="wrap_content"
            android:layout_height="609dp"
            android:layout_alignParentBottom="true"
            android:scrollbarSize="11dp"
            android:scrollbarStyle="insideOverlay"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="132dp" />
    </RelativeLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_first"
        style="@style/Widget.AppCompat.Button"
        android:layout_width="157dp"
        android:layout_height="60dp"
        android:layout_marginBottom="20dp"
        android:background="#FFFFFF"
        android:text="Dodaj produkt"
        android:textColor="#1877EC"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.969"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#D82121"
        tools:cornerRadius="5dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/buttonListOfProducts"
        android:layout_width="157dp"
        android:layout_height="60dp"
        android:layout_marginBottom="20dp"
        android:background="#FFFFFF"
        android:text="Lista ProduktÃ³w"
        android:textColor="#1877EC"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_first"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toStartOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>